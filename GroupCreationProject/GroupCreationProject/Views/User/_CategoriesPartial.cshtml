                <input type="checkbox" id="p2" name="p2" value="p2">
                <label for="p2">Preference 2</label><br>

                <input type="checkbox" id="p3" name="p3" value="p3">
                <label for="p3">Preference 3</label><br>
            </td>
            <td>
                <input type="checkbox" id="b1" name="b1" value="b1">
                <label for="b1">Background 1</label><br>

                <input type="checkbox" id="b2" name="b2" value="b2">
